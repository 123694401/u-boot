	iep@ff670000 {
		compatible = "rockchip,iep";
		iommu_enabled = <0x01>;
		iommus = <&iep_mmu>;
		reg = <0x00 0xff670000 0x00 0x800>;
		interrupts = <GIC_SPI 42 IRQ_TYPE_LEVEL_HIGH 0>;
		clocks = <&cru ACLK_IEP>,<&cru HCLK_IEP>; 
		clock-names = "aclk_iep", "hclk_iep";
		power-domains = <&power RK3399_PD_IEP>;
		allocator = <1>;
		version = <2>;
		status = "okay";
	}; 


