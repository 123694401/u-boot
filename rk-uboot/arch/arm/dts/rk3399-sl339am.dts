/* i2c0: i2c@ff3c0000 { */
&i2c0 {
	//compatible = "rockchip,rk3399-i2c";
	//reg = <0x00 0xff3c0000 0x00 0x1000>;
	//clocks = <&pmucru 0x09 0x30 0x1b>;
	//clock-names = "i2c", "pclk";
	//interrupts = <0x00 0x39 0x04 0x00>;
